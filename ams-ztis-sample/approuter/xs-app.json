{
  "welcomeFile": "/index.html",
  "logout": {
    "logoutEndpoint": "/logout",
    "logoutPage": "/logout.html"
  },
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/health",
      "target": "/health",
      "destination": "jakarta-ams-backend-destination",
      "authenticationType": "none"
    },
    {
      "source": "^/certificate",
      "target": "/certificate",
      "destination": "jakarta-ams-backend-destination",
      "authenticationType": "none"
    },
    {
      "source": "^/svid",
      "target": "/svid",
      "destination": "jakarta-ams-backend-destination",
      "authenticationType": "none"
    },
    {
      "source": "^/ams-jakarta-app/(.*)$",
      "target": "$1",
      "destination": "jakarta-ams-backend-destination",
      "authenticationType": "ias"
    },
    {
      "source": "^/logout.html$",
      "target": "logout.html",
      "localDir": "static",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)",
      "target": "$1",
      "localDir": "webapp",
      "authenticationType": "ias",
      "cacheControl": "no-cache, no-store, must-revalidate"
    }
  ]
}
