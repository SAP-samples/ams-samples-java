<mvc:View
    controllerName="monitor.monitor.monitor.Monitor"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>				
				<Page title="Java Security &amp; Authorization Management Service">
					<headerContent>
						<Button icon="sap-icon://log" text="Logout" press="onLogout" />
					</headerContent>
					<List class="sapUiSmallPadding">
						<items>
							<StandardListItem type="Navigation" press="onNavToLink"
								title="{links>/0/title}" description="{links>/0/text}" />
							<StandardListItem type="Navigation" press="onNavToLink"
								title="{links>/1/title}" description="{links>/1/text}" />
							<StandardListItem type="Navigation" press="onNavToLink"
								title="{links>/2/title}" description="{links>/2/text}" tooltip="{links>/2/description}" />
							<StandardListItem type="Navigation" press="onNavToAmsUi"
								title="{links>/3/title}" description="{links>/3/text}" />
							<StandardListItem type="Navigation" press="onNavToKnowledgeBase"
											  title="{links>/4/title}" description="{links>/4/text}" />
						</items>
					</List>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
